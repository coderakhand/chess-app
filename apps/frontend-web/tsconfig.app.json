{
   "extends": "@repo/typescript-config/react-library.json",
"compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",             
    "moduleResolution": "Bundler", 
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,     
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true
}
}
